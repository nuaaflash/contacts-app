<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.babytt.activity" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.babytt.activity.MainActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.babytt.activity.AddActivity"
            android:label="@string/title_activity_add"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.babytt.activity.DetailActivity"
            android:label="@string/title_activity_detail"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.babytt.activity.ModifyActivity"
            android:label="@string/title_activity_modify"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.babytt.activity.AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@android:style/Theme.Dialog"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
